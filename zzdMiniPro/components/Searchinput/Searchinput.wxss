.search_input {
    width: 100%;
    height: 90rpx;
    padding: 10rpx 10rpx 4rpx 10rpx;
    background-color: #1296db;
    position: fixed;
    top: 0;
    display: flex;
    justify-content: space-around;
}
.search_input navigator {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #fff;
    border-radius: 40rpx;
    color: #666;
}
